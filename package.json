{
  "name": "express-gen-ts",
  "version": "0.0.0",
  "scripts": {
    "build": "npx ts-node ./scripts/build.ts",
    "dev": "set NODE_ENV=development&& ts-node ./src",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src -e ts",
    "lint": "npx eslint ./src",
    "lint:tests": "npx eslint ./spec",
    "start": "NODE_ENV=production node -r ./preload.js ./dist",
    "test": "npx jest",
    "test:hot": "nodemon --exec \"npm run test\" --watch ./src --watch ./spec -e ts"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-generator-typescript": "^2.4.25",
    "find": "^0.3.0",
    "formidable": "^3.5.2",
    "fs-extra": "^11.2.0",
    "helmet": "^8.0.0",
    "inserturlparams": "^2.0.4",
    "jasmine": "^5.4.0",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.0.9",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mongoose": "^8.9.5",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "superagent": "^10.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.14.0",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-n": "^17.21.3",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.41.0"
  }
